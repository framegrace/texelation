{
  "clock": {
    "text_fg": "#f1fa8c"
  },
  "desktop": {
    "default_bg": "#282a36",
    "default_fg": "#f8f8f2"
  },
  "effects": {
    "bindings": [
      {
        "effect": "fadeTint",
        "event": "pane.active",
        "params": {
          "color": "#141400",
          "duration_ms": 400,
          "intensity": 0.35
        },
        "target": "pane"
      },
      {
        "effect": "rainbow",
        "event": "workspace.control",
        "params": {
          "mix": 0.6,
          "speed_hz": 0.5
        },
        "target": "workspace"
      },
      {
        "effect": "fadeTint",
        "event": "pane.resizing",
        "params": {
          "color": "#ffb86c",
          "duration_ms": 160,
          "intensity": 0.2
        },
        "target": "pane"
      },
      {
        "effect": "flash",
        "event": "workspace.key",
        "params": {
          "color": "#ffffff",
          "duration_ms": 250
        },
        "target": "workspace"
      }
    ]
  },
  "geometry": {
    "remove_duration_ms": 1000,
    "remove_effect": "ghost_n_grow",
    "split_duration_ms": 1000,
    "split_effect": "stretch",
    "zoom_duration_ms": 220,
    "zoom_effect": "expand"
  },
  "pane": {
    "active_border_bg": "#282a36",
    "active_border_fg": "#50fa7b",
    "effects": [
      {
        "color": "#141400",
        "duration_ms": 400,
        "id": "inactive-overlay",
        "intensity": 0.35
      },
      {
        "color": "#ffb86c",
        "duration_ms": 160,
        "id": "resizing-overlay",
        "intensity": 0.2
      }
    ],
    "inactive_border_bg": "#282a36",
    "inactive_border_fg": "#6272a4",
    "resizing_border_fg": "#ffb86c"
  },
  "selection": {
    "highlight_bg": "#e8d9ff",
    "highlight_fg": "#000000"
  },
  "statusbar": {
    "active_tab_bg": "#44475a",
    "active_tab_fg": "#f8f8f2",
    "base_bg": "#21222C",
    "base_fg": "#f8f8f2",
    "clock_fg": "#f1fa8c",
    "control_mode_bg": "#ff5555",
    "control_mode_fg": "#f8f8f2",
    "inactive_tab_bg": "#383a46",
    "inactive_tab_fg": "#6272a4",
    "title_fg": "#8be9fd"
  },
  "texelterm": {
    "long_line_editor_auto_open": true,
    "long_line_editor_enabled": true,
    "long_line_editor_width_threshold": 20,
    "reflow_enabled": true,
    "visual_bell_enabled": false,
    "wrap_enabled": true
  },
  "ui": {
    "caret_fg": "#c0c0c0",
    "focus_border_bg": "#000000",
    "focus_border_fg": "#ffff00",
    "focus_surface_bg": "#101010",
    "focus_surface_fg": "#ffffff",
    "focus_text_bg": "#101010",
    "focus_text_fg": "#ffffff",
    "overlay_bg": "#1e1e1e",
    "overlay_border": "#4a4a4a",
    "overlay_fg": "#f8f8f2",
    "surface_bg": "#000000",
    "surface_fg": "#f8f8f2",
    "text_bg": "#000000",
    "text_fg": "#f8f8f2"
  },
  "welcome": {
    "text_fg": "#bd93f9"
  },
  "workspace": {
    "effects": [
      {
        "id": "rainbow",
        "speed_hz": 0.5
      },
      {
        "color": "#ffffff",
        "duration_ms": 250,
        "id": "flash"
      }
    ]
  }
}